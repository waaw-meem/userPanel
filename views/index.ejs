<%- include('includes/head.ejs') %>
</head>
<body>
    <%- include('includes/navigation.ejs') %>


    <main>
        <h1>Home Page Welcome to add Users and Products</h1>


        <section>
            <article>
                <h1>Users List</h1>
             
                 <div class="user_grid-container">
                   
                    <div class="user_card">
                        <div class="user_avatar">
                            <img src="data:image/jpeg;base64," alt="image" class="user_avatar">
                        </div>
                        <div class="user_details">
                            <span class="user_name">Wali</span>
                            <span class="user_status">Software Developer</span>
                        </div>
                        <button>See Details</button>
                    </div>
                 
                </div>
              
               </article>
        </section>

         <section>
            <article>
                <h1>Product List</h1>
                <% if( prods.length > 0 ) { %>
                 <div class="prod_grid-container">
                    <% for(let product of prods) { %>
                        <div class="prod_card">
                            <div class="prod_avatar">
                                <img src="data:image/jpeg;base64," alt="image" class="prod_avatar">
                            </div>
                            <div class="prod_details">
                                <span class="prod_name"><%= product.title %></span>
                                <span class="prod_description">Washing Machine</span>
                            </div>
                            <div class="prod_price">
                                <span>$23</span>
                            </div>
                            <button>See Details</button>
                        </div>
                    <% } %>    
                </div>
                <% } else { %>
                    <h1>Page Not Found!!!</h1>
                <% } %>    
               </article>
         </section>

    </main>
    <%- include('includes/end.ejs') %>